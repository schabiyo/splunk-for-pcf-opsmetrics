  <row>
    <panel>
      <single>
        <title>Router Req/s (5m)</title>
        <search>
          <query>index=main sourcetype=jmx host="*" opentsdb.nozzle.router__0.total_requests=* jvmDescription mbean_domain="org.cloudfoundry" mbean_property_deployment="cf" mbean_property_job="router-*" | eval job_and_index = jvmDescription + "_" + mbean_property_job + "_" + mbean_property_index + "_" + mbean_property_ip | rename "opentsdb.nozzle.router__0.total_requests" as total_requests | stats sum(total_requests) as sum_total_requests by _time job_and_index | eval requests_per_second =  sum_total_requests * 0.001 | table requests_per_second</query>
          <earliest>rt-5m</earliest>
          <latest>rtnow</latest>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">block</option>
        <option name="numberPrecision">0</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="useColors">1</option>
        <option name="useThousandSeparators">1</option>
        <option name="rangeColors">["0x65a637","0x6db7c6","0xf7bc38","0x65a637","0x65a637"]</option>
        <option name="rangeValues">[0,30,70,100]</option>
        <option name="trendInterval">auto</option>
        <option name="underLabel">per second</option>
        <option name="linkView">search</option>
        <option name="drilldown">none</option>
        <option name="unitPosition">after</option>
      </single>
    </panel>
  </row>
